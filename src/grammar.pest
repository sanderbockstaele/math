alpha = { 'a'..'z' | 'A'..'Z' }

digit = { '0'..'9' }

simple_operations = {"+" | "-" | "*" | "/"}

function = { alpha+ ~ "(" ~ argument ~ ")" }

argument = { alpha | digit+ | function }

equation_header = { function ~ "=" }

expression = { argument ~ simple_operations ~ argument }

equation = { equation_header ~ expression+ }

input = { equation | expression }